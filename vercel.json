{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/bplanjr",
      "dest": "/bplanjr/index.html"
    },
    {
      "src": "/bplanjr/(.*)",
      "dest": "/bplanjr/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}