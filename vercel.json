{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "."
      }
    },
    {
      "src": "bplanjr/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "bplanjr/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/bplanjr",
      "dest": "/bplanjr/dist/index.html"
    },
    {
      "src": "/bplanjr/(.*)",
      "dest": "/bplanjr/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}